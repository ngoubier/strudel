setcpm(117/4)
samples({
  bd: 'https://ngoubier.github.io/strudel/samples/drums/bassdrum/kick-billie-jean_117bpm_C_major.wav',
  hihat: 'https://raw.githubusercontent.com/tidalcycles/Dirt-Samples/master/hh27/000_hh27closedhh.wav',
  sd: ['https://ngoubier.github.io/strudel/samples/drums/snare/bright-snare-billie-jean_C.wav'],
});

$:s("bd sd bd sd, hihat*8")

$:note("f#2 c#2 e2 f#2 e2 c#2 b1 c#2, f#1 [-]") 
  .s("sawtooth")    
  .attack(0.005)  
  .decay(0.15)
  .sustain(0.1)
  .release(0.0)
  .cutoff(300)    // filtre passe-bas â†’ plus chaud
  .gain(0.9)
  .lpf("200")
  